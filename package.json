{
  "name": "production-microservice",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "node -r dotenv/config dist/main/http/index.js",
    "dev": "ts-node-dev --respawn --transpile-only --clear --ignore-watch node_modules -r dotenv/config src/main/http/index.ts dotenv_config_path=.env",
    "clean": "rimraf dist",
    "build": "npm run clean && tsc -p tsconfig.build.json",
    "check": "npx npm-check -s -u",
    "lint": "eslint .",
    "lint:fix": "npm run lint -- --fix",
    "test": "jest --passWithNoTests --runInBand --no-cache --detectOpenHandles",
    "test:watch": "npm t -- --watch",
    "test:staged": "npm t -- --findRelatedTests",
    "test:coverage": "npm t -- --coverage",
    "test:bdd": "cucumber-js"
  },
